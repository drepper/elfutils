[Unit]
Description=
Documentation=http://elfutils.org/
After=network.target

[Service]
EnvironmentFile=/etc/sysconfig/@program_prefix@dbgserver
User=eu-dbgserver
Group=eu-dbgserver
#CacheDirectory=dbgserver
ExecStart=/usr/bin/@program_prefix@dbgserver -d /var/cache/@program_prefix@dbgserver/dbgserver.sqlite -p $DBGSERVER_PORT $DBGSERVER_VERBOSE $DBGSERVER_PATHS
TimeoutStopSec=10

[Install]
WantedBy=multi-user.target
